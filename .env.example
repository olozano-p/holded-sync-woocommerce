# Holded API
HOLDED_API_KEY=your_holded_api_key_here

# Secondary Holded Account (optional - for routing specific SKUs)
HOLDED_SECONDARY_API_KEY=your_secondary_holded_api_key_here
HOLDED_SECONDARY_SKUS=CasaVirupa-231,CasaVirupa-324,LAC-221
EXCLUDED_SKUS=CASAVIRUPA-2397,CASAVIRUPA-2396
HOLDED_SECONDARY_VAT_RATE=10

# SumUp API
SUMUP_API_KEY=your_sumup_api_key_here
SUMUP_DEFAULT_VAT_RATE=21
# Alternative: OAuth tokens
# SUMUP_CLIENT_ID=your_client_id
# SUMUP_CLIENT_SECRET=your_client_secret

# WooCommerce Site 1 (e.g., ICTUS)
WC_SITE1_URL=https://your-first-site.com
WC_SITE1_KEY=ck_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE1_SECRET=cs_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE1_PREFIX=ICTUS
WC_SITE1_DEFAULT_VAT_RATE=21

# WooCommerce Site 2 (e.g., Alumbra)
WC_SITE2_URL=https://your-second-site.com
WC_SITE2_KEY=ck_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE2_SECRET=cs_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE2_PREFIX=ALUM
WC_SITE2_DEFAULT_VAT_RATE=21

# WooCommerce Site 3 (e.g., LAC)
WC_SITE3_URL=https://your-third-site.com
WC_SITE3_KEY=ck_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE3_SECRET=cs_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
WC_SITE3_PREFIX=LAC
WC_SITE3_DEFAULT_VAT_RATE=10

# Sync Settings
SYNC_DAYS_BACK=1
DEFAULT_VAT_RATE=21        # Global fallback VAT rate (used if site-specific rates not set)
LOG_LEVEL=info

# Holded Document Settings
# Document type: invoice, salesreceipt, creditnote, salesorder, proform
HOLDED_DOC_TYPE=invoice
# Invoice numbering format (e.g., F[YY]%%%% = F25001, F25002...)
HOLDED_NUMBERING_FORMAT=F[YY]%%%%
# Default sales channel name (must exist in Holded)
HOLDED_SALES_CHANNEL=
# Default warehouse ID (get from Holded)
HOLDED_WAREHOUSE=

# Timezone (for scheduling)
TZ=Europe/Madrid
